<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.co.genuine.hm.repository.user.UserAccountRepository">
   <select id="findOne" resultType="jp.co.genuine.hm.model.user.UserAccount">
      SELECT account_id AS accountId, password FROM "user".account WHERE account_id = #{accountId};
   </select>
</mapper>